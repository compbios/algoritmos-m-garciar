**Números aleatorios**

Algoritmo sin_titulo

	definir vector,x como Entero
  
	Dimension vector[12]
  
	Para x = 1 hasta 12 Hacer
  
		vector[x] = 1 + azar[10]
    
		Escribir vector[x]
    
	FinPara
  
  FinAlgoritmo
  
  **Promedio notas 1 estudiante**
  
Algoritmo sin_titulo

	Definir notas, n, suma Como Real
  
	Dimension notas[5]
  
	suma<-0
  
	Para n <- 1 hasta 5 Hacer
  
 	Escribir "Digite sus notas", n
  
	Leer notas[n]
  
	suma<-suma+notas[n]
  
FinPara

Escribir "Su promedio es: "

Escribir  suma/5

Si suma/5>=3 Entonces

	Escribir "Aprobó"
  
SiNo

	Escribir "No aprobó"
  
Fin Si

Si suma/5>=4 Entonces

	Escribir "Desempeño sobresaliente"
  
SiNo

	Escribir " "
  
Fin Si

FinAlgoritmo

**Promedio 4 estudiantes**

Algoritmo sin_titulo

	Dimension notas[5,4]
  
	Escribir "Estudiante 1 digite sus calificaciones"
  
	Leer notas[1,1]
  
	Leer notas[2,1]
  
	Leer notas[3,1]
  
	Leer notas[4,1]
  
	Leer notas[5,1]
  
	Escribir "Promedio del estudiante 1"
  
	Escribir (notas[1,1] + notas[2,1] + notas[3,1] + notas[4,1] +notas[5,1])/5
  
	Escribir "Estudiante 2 digite sus calificaciones"
  
	Leer notas[1,2]
  
	Leer notas[2,2]
  
	Leer notas[3,2]
  
	Leer notas[4,2]
  
	Leer notas[5,2]
  
	Escribir "Promedio del estudiante 2"
  
	Escribir (notas[1,2] + notas[2,2] + notas[3,2] + notas[4,2] +notas[5,2])/5
  
	Escribir "Estudiante 3 digite sus calificaciones"
  
	Leer notas[1,3]
  
	Leer notas[2,3]
  
	Leer notas[3,3]
  
	Leer notas[4,3]
  
	Leer notas[5,3]
  
	Escribir "Promedio del estudiante 3"
  
	Escribir (notas[1,3] + notas[2,3] + notas[3,3] + notas[4,3] +notas[5,3])/5
  
	Escribir "Estudiante 4 digite sus calificaciones"
  
	Leer notas[1,4]
  
	Leer notas[2,4]
  
	Leer notas[3,4]
  
	Leer notas[4,4]
  
	Leer notas[5,4]
  
	Escribir "Promedio del estudiante 4"
  
	Escribir (notas[1,4] + notas[2,4] + notas[3,4] + notas[4,4] +notas[5,4])/5

FinAlgoritmo


